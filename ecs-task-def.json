{
  "containerDefinitions":[
    {
      "name":"<PROJECT_NAME>",
      "image":"<ECR_REGISTRY>/<ORG>/<PROJECT_NAME>:<REVISION>",
      "portMappings":[
        {
          "containerPort":8080,
          "hostPort":8080
        },
        {
          "containerPort":8081,
          "hostPort":8081
        },
        {
          "containerPort":8082,
          "hostPort":8082
        }
      ],
      "environment":[
        {
          "name":"DISCOVERY_CONFIG",
          "value":"{\"Env:DISCOVERY_EXPIRATION\": \"1800\", \"Env:STATUS_TIME\": \"600\"}"
        },
        {
          "name":"STATIC_PATH",
          "value":"/static"
        },
        {
          "name":"USE_SWAGGER",
          "value":"false"
        }
      ],
			"memory":100,
			"logConfiguration": {
				"logDriver": "awslogs",
				"options": {
					"awslogs-group": "<AWS_LOG_GROUP>",
					"awslogs-region": "<AWS_LOG_REGION>",
					"awslogs-stream-prefix": "<AWS_LOG_STREAM_PREFIX>"
				}
			}
		}
	],
  "family":"<ECS_TASK_FAMILY>"
}

