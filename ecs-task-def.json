{
  "containerDefinitions":[
    {
      "name":"<PROJECT_NAME>",
      "image":"<ECR_REGISTRY>/<ORG>/<PROJECT_NAME>:<REVISION>",
      "portMappings":[
        {
          "containerPort":8080,
          "hostPort":8080
        },
        {
          "containerPort":8081,
          "hostPort":8081
        },
        {
          "containerPort":8082,
          "hostPort":8082
        }
      ],
      "environment":[ 
				{
					"name": "VASCO_PROXY",
					"value": "8080"
				},
				{
					"name": "VASCO_REGISTRY",
					"value": "8081"
				},
				{
					"name": "VASCO_STATUS",
					"value": "8082"
				},
				{
					"name": "REDIS_ADDR",
					"value": "<REDIS_ADDR>"
				},
				{
					"name": "MINPORT",
					"value": "8100"
				},
				{
					"name": "MAXPORT",
					"value": "9900"
				},
				{
					"name": "EXPECTED_SERVICES",
					"value": ""
				},
				{
					"name": "STATUS_TIME",
					"value": "60"
				},
        {
          "name":"DISCOVERY_EXPIRATION",
          "value": "3600"
        },
        {
          "name":"STATIC_PATH",
          "value": ""
        },
        {
          "name":"USE_SWAGGER",
          "value": "false"
        }
      ],
			"memory":100,
			"logConfiguration": {
				"logDriver": "awslogs",
				"options": {
					"awslogs-group": "<AWS_LOG_GROUP>",
					"awslogs-region": "<AWS_LOG_REGION>",
					"awslogs-stream-prefix": "<AWS_LOG_STREAM_PREFIX>"
				}
			}
		}
	],
  "family":"<ECS_TASK_FAMILY>"
}

