{
  "containerDefinitions":[
    {
      "name":"<PROJECT_NAME>",
      "image":"<ECR_REGISTRY>/<ORG>/<PROJECT_NAME>:<REVISION>",
      "portMappings":[
        {
          "containerPort":<VASCO_PROXY>,
          "hostPort":<VASCO_PROXY>
        },
        {
          "containerPort":<VASCO_REGISTRY>,
          "hostPort":<VASCO_REGISTRY>
        },
        {
          "containerPort":<VASCO_STATUS>,
          "hostPort":<VASCO_STATUS>
        }
      ],
      "environment":[ 
				{
					"name": "VASCO_PROXY",
					"value": "<VASCO_PROXY>"
				},
				{
					"name": "VASCO_REGISTRY",
					"value": "<VASCO_REGISTRY>"
				},
				{
					"name": "VASCO_STATUS",
					"value": "<VASCO_STATUS>"
				},
				{
					"name": "REDIS_ADDR",
					"value": "<REDIS_ADDR>"
				},
				{
					"name": "MINPORT",
					"value": "8100"
				},
				{
					"name": "MAXPORT",
					"value": "9900"
				},
				{
					"name": "EXPECTED_SERVICES",
					"value": "config assess item passage pdf sas staticserver stdmirror stdtag user assessapi"
				},
				{
					"name": "STATUS_TIME",
					"value": "60"
				},
        {
          "name":"DISCOVERY_EXPIRATION",
          "value": "3600"
        },
        {
          "name":"STATIC_PATH",
          "value": ""
        },
        {
          "name":"USE_SWAGGER",
          "value": "false"
        }
      ],
			"memory":100,
			"logConfiguration": {
				"logDriver": "awslogs",
				"options": {
					"awslogs-group": "<AWS_LOG_GROUP>",
					"awslogs-region": "<AWS_LOG_REGION>",
					"awslogs-stream-prefix": "<AWS_LOG_STREAM_PREFIX>"
				}
			}
		}
	],
  "family":"<ECS_TASK_FAMILY>"
}

